[remote "numpy"]
    url = git@github.com:numpy/numpy.git
    fetch = +refs/heads/*:refs/remotes/numpy/*
[remote "github"]
    url = git@github.com:huangyingw/numpy.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = numpy
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = numpy
    merge = refs/heads/master
[commit]
    message = n
